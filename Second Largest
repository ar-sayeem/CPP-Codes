#include <iostream>
using namespace std;

int main()
{
    int T;
    cin >> T;
    while(T--)
    {
        int A, B, C;
        cin>> A >> B >> C;
        if(A>B && A<C || A<B && A>C)            // Greater than one but less than another.
        {
            cout << A << endl;
        }
        else if(B>A && B<C || B<A && B>C)            // Greater than one but less than another.
        {
            cout << B << endl;
        }
        else                                 // The other other one.
        {
            cout << C << endl;
        }
    }
    return 0;
}
