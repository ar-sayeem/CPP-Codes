#include <iostream>
using namespace std;

int main()
{
    int T;
    cin >> T;       // User input 'T' for test case.
    while(T--)
    {
        int N, M;           // N = required run, M = remaining over.
        cin >> N >> M;
        if( N <= M*6*6 )        // One over = 6 balls, One ball = maximum 6 runs.
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
    return 0;
}
