#include <iostream>
using namespace std;

int main() {
    int T;
    cin >> T;         // User input 'T' for test case.
    while(T--)
    {
        int X, Y;
        cin >> X >> Y;          // X=litre, Y=distance.
        if(X*5 >= Y)      // Travel up to 5 kilometres on 1 litre of fuel.
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
	return 0;
}
