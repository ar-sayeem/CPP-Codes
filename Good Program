#include <iostream>
using namespace std;

int main()
{
    int t;
    cin>>t;         // User input 't' for test case.
    while(t--)
    {
        int N;
        cin>>N;         // User input 'N' for bits of memory.
        if(N%4 == 0)          // Checks whether N % 4 is equivalent to 0.
            cout << "Good" << endl;         // Condition satisfied
        else
            cout << "Not Good" << endl;         // Condition dissatisfied.
    }
    return 0;
}
