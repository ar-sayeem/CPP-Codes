#include <bits/stdc++.h>    // "bits/stdc++.h" is a header file that include all the standard libraries.
using namespace std;

int main()
{
    cout.precision(2);  // setprecision() function specifies the number of decimal places to be printed in the output.

    int X;      // Amount to withdraw.
    float Y;        // Pooja's balance.
    cin >> X >> Y;

    if ( (X%5==0) && (Y-X-0.5>=0) )     // Multiple of 5 and enough balance to withdraw.
    {
        cout << fixed << Y-X-0.5 << endl;
    }
    else
    {
        cout << fixed << Y << endl;
    }
    return 0;
}
