#include<bits/stdc++.h>  // "bits/stdc++.h" is a header file that include all the standard libraries.
using namespace std;

int main()
{
    int N;
    cin >> N;           // User input N = total people.
    if( N<=8 && N>=6 )          // This hunt is intended for teams of 6 to 8 people.
        cout << "YES" << endl;
    else
        cout << "NO" << endl;
    return 0;
}
