#include <iostream>
using namespace std;

int main()
{
    int T;      // Test case.
    cin>>T;
    while(T--)
    {
        int A;
        cin >> A;            // Input a integer number.

        if(A%2==0 && A%7==0)        // Divisible by 2 means even and multiple of 7.
            cout << "Alice" << endl;
        else if(A%2!=0 && A%9==0)         // Not divisible by 2 means odd and multiple of 9.
            cout << "Bob" << endl;
        else
            cout << "Charlie" << endl;
    }
    return 0;
}
