#include <iostream>
using namespace std;

int main() {

    int T;
    cin >> T;      // User input 'T' for test case.
    while(T--)
    {
        int X, Y;       // X = Following, Y = Followers.
        cin >> X >> Y;
        if(X > Y*10)         // If the account's following count exceeds ten times the number of followers.
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
	return 0;
}
