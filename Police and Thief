#include <bits/stdc++.h>     // "bits/stdc++.h" is a header file that include all the standard libraries.
using namespace std;

int main()
{
    int T;          // User input 'T' for test case.
    cin >> T;
    while(T--)
    {
        int X, Y;       // X = Police location, Y = Thief location.
        cin >> X >> Y;
        cout << abs(X-Y) << endl;       // Because their relative speed is one unit per second,
                                       // time will equal the absolute distance between them.
    }
    return 0;
}
